{
	"name": "@westpac/pictogram",
	"version": "1.0.0",
	"description": "Pictogram component for the Westpac GEL",
	"repository": "https://github.com/WestpacGEL/GEL/tree/master/components/pictogram",
	"blender": {
		"recipe": "blender/recipe.js"
	},
	"author": "Westpac GEL Team <gel@westpac.com.au>",
	"contributors": [
		{
			"name": "Jonathan Stening",
			"email": "info@jonathanstening.com",
			"url": "http://jonathanstening.com"
		},
		{
			"name": "Jeremy Ortiz",
			"email": "jeremy@jeremyortiz.io",
			"url": "https://www.jeremyortiz.io/"
		}
	],
	"license": "GPL-3.0+",
	"scripts": {
		"start": "PACKAGE_NAME=pictogram webpack-dev-server --config ../../helpers/example/start.webpack.config.js",
		"test": "exit 0 # start-server-and-test start http://localhost:8080 test:all",
		"test:dev": "start-server-and-test start http://localhost:8080 test:test-dev",
		"test:all": "yarn test:integration",
		"test:integration": "cypress run",
		"test:test-dev": "cypress open",
		"build": "node ../../helpers/buildExports.js export informative decorative"
	},
	"main": "dist/pictogram.cjs.js",
	"module": "dist/pictogram.esm.js",
	"sideEffects": false,
	"preconstruct": {
		"entrypoints": [
			".",
			"informative/AccessibilityPictogram",
			"informative/BirthCertificatePictogram",
			"informative/BusinessPersonPictogram",
			"informative/CalculatorPictogram",
			"informative/CalendarPictogram",
			"informative/CardLockedPictogram",
			"informative/ChatPictogram",
			"informative/CoinsPictogram",
			"informative/CreditCardPictogram",
			"informative/DesktopComputerPictogram",
			"informative/DollarSignPictogram",
			"informative/DriversLicencePictogram",
			"informative/GenericDocumentPictogram",
			"informative/GiftPictogram",
			"informative/HousePictogram",
			"informative/LocationPinPictogram",
			"informative/MedicareCardPictogram",
			"informative/MobileDevicePictogram",
			"informative/MoneyInPictogram",
			"informative/MoneyOutPictogram",
			"informative/Number1Pictogram",
			"informative/Number2Pictogram",
			"informative/Number3Pictogram",
			"informative/Number4Pictogram",
			"informative/Number5Pictogram",
			"informative/PadlockLockedPictogram",
			"informative/PadlockUnlockedPictogram",
			"informative/PassportPictogram",
			"informative/PercentSignPictogram",
			"informative/PersonPictogram",
			"informative/StarPictogram",
			"informative/StopwatchPictogram",
			"informative/TelephoneCallPictogram",
			"informative/ThumbsUpPictogram",
			"informative/TickPictogram",
			"informative/TruckPictogram",
			"informative/WalletPictogram",
			"decorative/WBCBankCardPictogram"
		]
	},
	"dependencies": {
		"@babel/runtime": "^7.9.6",
		"@westpac/core": "^1.0.1",
		"prop-types": "^15.7.2"
	},
	"peerDependencies": {
		"react": "^16.13.1"
	},
	"devDependencies": {
		"@westpac/grid": ">1.0.0-alpha",
		"cypress": "^4.5.0",
		"react": "^16.13.1",
		"start-server-and-test": "^1.11.0"
	}
}
